{
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "clean": "turbo run clean",
    "coverage": "turbo run coverage",
    "cs": "changeset",
    "depcheck": "turbo run depcheck",
    "dev": "turbo run dev --parallel",
    "postinstall": "husky install",
    "just-func": "yarn workspace just-func",
    "lint": "turbo run lint",
    "nuke": "turbo run nuke",
    "nuke:local": "rimraf node_modules || true",
    "prepack": "pinst --disable",
    "postpack": "pinst --enable",
    "prepare": "husky install",
    "size": "turbo run size",
    "test": "turbo run test",
    "verify": "turbo run lint build coverage depcheck size",
    "version": "changeset version && yarn install",
    "watch": "jest --watch"
  },
  "devDependencies": {
    "@babel/core": "^7.18.2",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.17.12",
    "@babel/plugin-proposal-optional-chaining": "^7.17.12",
    "@babel/preset-env": "^7.18.2",
    "@babel/preset-typescript": "^7.17.12",
    "@changesets/cli": "^2.22.0",
    "@commitlint/cli": "^17.0.2",
    "@commitlint/config-conventional": "^17.0.2",
    "@types/jest": "^28.1.1",
    "@types/ms": "^0.7.31",
    "@types/ramda": "^0.28.13",
    "@types/react": "^18.0.11",
    "@types/semver": "^7.3.9",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "assertron": "^9.2.3",
    "babel-jest": "^28.1.0",
    "babel-loader": "^8.2.5",
    "delay": "^5.0.0",
    "depcheck": "^1.4.3",
    "eslint": "^8.17.0",
    "eslint-plugin-harmony": "^7.1.0",
    "husky": "^8.0.1",
    "jest": "^28.1.0",
    "jest-watch-suspend": "^1.1.2",
    "jest-watch-toggle-config": "^2.0.1",
    "jest-watch-typeahead": "^1.1.0",
    "lerna": "^5.0.0",
    "npm-check-updates": "^13.1.1",
    "npm-run-all": "^4.1.5",
    "pinst": "^3.0.0",
    "ramda": "^0.28.0",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "rimraf": "^3.0.2",
    "satisfier": "^5.2.0",
    "turbo": "^1.2.16",
    "typescript": "^4.7.3"
