{
  "name": "standard-log",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "bootstrap": "lerna bootstrap",
    "build": "tsc -b",
    "build-storybook": "build-storybook",
    "build:watch": "tsc -b -w",
    "bundle": "lerna run bundle",
    "clean": "lerna run clean && rimraf .tsbuildinfo",
    "codacy": "cat coverage/lcov.info | codacy-coverage",
    "coverage": "jest --coverage",
    "depcheck": "lerna run depcheck",
    "lint": "eslint --ext=js,ts .",
    "nuke": "yarn clean && lerna clean -y && rimraf node_modules",
    "storybook": "start-storybook -p 6006",
    "test": "jest",
    "test:nodejs": "jest",
    "test:watch": "jest --watch",
    "verify": "yarn lint && yarn build && yarn depcheck && yarn coverage && yarn bundle",
    "watch": "yarn test:watch"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.12.1",
    "@babel/plugin-proposal-optional-chaining": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-typescript": "^7.12.1",
    "@changesets/cli": "^2.11.1",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@storybook/addon-actions": "^6.0.27",
    "@storybook/addon-links": "^6.0.27",
    "@storybook/react": "^6.0.27",
    "@types/jest": "^26.0.15",
    "@types/ms": "^0.7.31",
    "@types/ramda": "^0.27.30",
    "@types/react": "^16.9.54",
    "@types/semver": "^7.3.4",
    "assertron": "^7.1.3",
    "babel-jest": "^24",
    "babel-loader": "^8.1.0",
    "codacy-coverage": "^3.4.0",
    "codecov": "^3.8.0",
    "commitlint-circle": "^1.0.0",
    "compression-webpack-plugin": "^6.0.4",
    "console-feed": "^3.1.10",
    "delay": "^4.4.0",
    "dependency-check": "^4.1.0",
    "eslint": "^7.12.1",
    "eslint-plugin-harmony": "^5.0.1",
    "husky": "^4.3.0",
    "jest": "^24",
    "jest-audio-reporter": "^2.2.1",
    "jest-progress-tracker": "^2.0.2",
    "jest-watch-repeat": "^2.0.0",
    "jest-watch-suspend": "^1.1.2",
    "jest-watch-toggle-config": "^2.0.1",
    "jest-watch-typeahead": "^0.6.1",
    "lerna": "^3.22.1",
    "npm-check-updates": "^9.2.3",
    "param-case": "^3.0.3",
    "pascal-case": "^3.1.1",
    "prop-types": "^15.7.2",
    "ramda": "^0.27.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "rimraf": "^3.0.2",
    "satisfier": "^5.1.0",
    "source-map-loader": "^1.1.2",
    "ts-loader": "^8.0.7",
    "typescript": "^4.0.5",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^5.3.0",
    "webpack-cli": "^4.1.0"
  }
}
