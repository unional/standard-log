{
  "name": "standard-log",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "bootstrap": "lerna bootstrap",
    "build": "tsc -b",
    "build-storybook": "build-storybook",
    "build:watch": "tsc -b -w",
    "bundle": "lerna run bundle",
    "clean": "lerna run clean && rimraf .tsbuildinfo",
    "codacy": "cat coverage/lcov.info | codacy-coverage",
    "coverage": "jest --coverage",
    "depcheck": "lerna run depcheck",
    "lint": "eslint --ext=js,ts .",
    "nuke": "yarn clean && lerna clean -y && rimraf node_modules",
    "storybook": "start-storybook -p 6006",
    "test": "jest",
    "test:nodejs": "jest",
    "test:watch": "jest --watch",
    "verify": "yarn lint && yarn build && yarn depcheck && yarn coverage && yarn bundle",
    "watch": "yarn test:watch"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "devDependencies": {
    "@babel/core": "7.13.1",
    "@babel/plugin-proposal-nullish-coalescing-operator": "7.13.0",
    "@babel/plugin-proposal-optional-chaining": "7.13.0",
    "@babel/preset-env": "7.13.5",
    "@babel/preset-typescript": "7.13.0",
    "@changesets/cli": "2.14.1",
    "@commitlint/cli": "11.0.0",
    "@commitlint/config-conventional": "11.0.0",
    "@storybook/addon-actions": "6.1.20",
    "@storybook/addon-links": "6.1.20",
    "@storybook/react": "6.1.20",
    "@types/jest": "^26.0.15",
    "@types/ms": "0.7.31",
    "@types/ramda": "0.27.38",
    "@types/react": "16.14.4",
    "@types/semver": "7.3.4",
    "assertron": "7.1.3",
    "babel-jest": "24.9.0",
    "babel-loader": "8.2.2",
    "codacy-coverage": "3.4.0",
    "codecov": "3.8.1",
    "commitlint-circle": "1.0.0",
    "compression-webpack-plugin": "6.1.1",
    "console-feed": "3.2.2",
    "delay": "4.4.1",
    "dependency-check": "4.1.0",
    "eslint": "7.20.0",
    "eslint-plugin-harmony": "5.0.2",
    "husky": "4.3.8",
    "jest": "24.9.0",
    "jest-audio-reporter": "2.2.1",
    "jest-progress-tracker": "2.0.2",
    "jest-watch-repeat": "2.0.0",
    "jest-watch-suspend": "1.1.2",
    "jest-watch-toggle-config": "2.0.1",
    "jest-watch-typeahead": "0.6.1",
    "lerna": "3.22.1",
    "npm-check-updates": "9.2.4",
    "param-case": "3.0.4",
    "pascal-case": "3.1.2",
    "ramda": "0.27.1",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "rimraf": "3.0.2",
    "satisfier": "5.1.0",
    "source-map-loader": "1.1.3",
    "ts-loader": "8.0.17",
    "typescript": "4.2.2",
    "uglifyjs-webpack-plugin": "2.2.0",
    "webpack": "5.24.1",
    "webpack-cli": "4.5.0"
  }
}
