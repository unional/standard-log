{
  "name": "standard-log-console",
  "version": "1.1.0",
  "description": "Console writer for `standard-log`",
  "homepage": "https://github.com/unional/standard-log",
  "bugs": {
    "url": "https://github.com/unional/standard-log/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/unional/standard-log.git"
  },
  "license": "MIT",
  "author": {
    "name": "Homa Wong",
    "email": "homawong@gmail.com"
  },
  "files": [
    "dist",
    "lib"
  ],
  "main": "lib/index.js",
  "module": "libm/index.js",
  "jsnext:main": "libm/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "build": "tsc -p tsconfig.lib.json && tsc -p tsconfig.libm.json",
    "bundle": "webpack",
    "clean": "rimraf dist lib libm",
    "dc": "yarn dependency-check",
    "dependency-check": "dependency-check . --unused --no-dev && dependency-check . --missing --no-dev",
    "test": "jest",
    "verify": "yarn clean && yarn test -- --reporters=default && yarn build && yarn dc && yarn bundle",
    "preversion": "yarn verify",
    "watch": "jest --watch"
  },
  "dependencies": {
    "ansi-colors": "^4.1.1",
    "color-map": "^1.1.0",
    "is-node": "^1.0.2",
    "ms": "^2.1.2",
    "semver": "^6.2.0",
    "standard-log-core": "^1.1.1",
    "type-plus": "^1.26.2",
    "upper-case": "^1.1.3"
  },
  "devDependencies": {
    "@types/ms": "^0.7.30",
    "@types/semver": "^6.0.1"
  }
}
