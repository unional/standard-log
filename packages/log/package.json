{
  "name": "standard-log",
  "version": "1.5.2",
  "description": "The standard log library",
  "homepage": "https://github.com/unional/standard-log",
  "bugs": {
    "url": "https://github.com/unional/standard-log/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/unional/standard-log.git"
  },
  "license": "MIT",
  "author": {
    "name": "Homa Wong",
    "email": "homawong@gmail.com"
  },
  "files": [
    "dist",
    "lib",
    "libm"
  ],
  "main": "lib/index.js",
  "module": "libm/index.js",
  "jsnext:main": "libm/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "build": "tsc -p tsconfig.lib.json && tsc -p tsconfig.libm.json",
    "bundle": "webpack",
    "clean": "rimraf dist lib libm",
    "dc": "yarn dependency-check",
    "dependency-check": "dependency-check . --unused --no-dev -i standard-log-core && dependency-check . --missing --no-dev",
    "lint": "tslint -p tsconfig.json",
    "prepublishOnly": "yarn build && yarn bundle",
    "test": "jest",
    "verify": "yarn lint && yarn build && yarn dc && yarn test --reporters=default && yarn bundle",
    "watch": "jest --watch"
  },
  "dependencies": {
    "global-store": "^0.8.2",
    "is-node": "^1.0.2",
    "iso-error": "^2.2.0",
    "ms": "^2.1.2",
    "standard-log-core": "^1.5.0",
    "type-plus": "^1.26.2",
    "upper-case": "^1.1.3"
  },
  "devDependencies": {
    "@types/ramda": "^0.26.16",
    "assertron": "^7.1.2",
    "ramda": "^0.26.1",
    "satisfier": "^5.0.2"
  }
}
