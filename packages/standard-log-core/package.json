{
  "name": "standard-log-core",
  "version": "1.1.1",
  "description": "Core library for standard-log",
  "homepage": "https://github.com/unional/logging",
  "bugs": {
    "url": "https://github.com/unional/logging/issues"
  },
  "repository": {
    "type": "git",
