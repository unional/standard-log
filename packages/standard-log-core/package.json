    "name": "Homa Wong",
    "email": "homawong@gmail.com"
  },
  "files": [
    "dist",
    "lib"
  ],
  "main": "lib/index.js",
  "module": "libm/index.js",
  "jsnext:main": "libm/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "build": "tsc -p tsconfig.lib.json && tsc -p tsconfig.libm.json",
    "clean": "rimraf lib libm",
    "dc": "yarn dependency-check",
    "dependency-check": "dependency-check . --unused --no-dev && dependency-check . --missing --no-dev",
    "test": "jest",
    "verify": "yarn clean && yarn test -- --reporters=default && yarn build && yarn dc",
    "preversion": "yarn verify",
    "watch": "jest --watch"
  },
  "dependencies": {
    "global-store": "^0.8.1"
  },
  "devDependencies": {
    "@types/ramda": "^0.25.48",
    "assertron": "^6.1.0",
    "ramda": "^0.26.1"
  }
}
