{
  "name": "@standard-log/core",
  "version": "3.0.1",
  "description": "Core library for `standard-log`",
  "homepage": "https://github.com/unional/standard-log",
  "bugs": {
    "url": "https://github.com/unional/standard-log/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/unional/standard-log.git"
  },
  "license": "MIT",
  "author": {
    "name": "Homa Wong",
    "email": "homawong@gmail.com"
  },
  "files": [
    "dist",
    "lib",
    "libm"
  ],
  "main": "lib/index.js",
  "module": "libm/index.js",
  "jsnext:main": "libm/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "_dc": "yarn dependency-check",
    "build": "tsc -p tsconfig.lib.json && tsc -p tsconfig.libm.json",
    "clean": "rimraf dist lib libm",
    "coverage": "jest --coverage",
    "dependency-check": "dependency-check . --unused --no-dev && dependency-check . --missing --no-dev",
    "lint": "tslint -p tsconfig.json",
    "prepublishOnly": "yarn build",
    "test": "jest",
    "test:watch": "jest --watch",
    "verify": "yarn lint && yarn dc && yarn coverage",
    "watch": "jest --watch"
  },
  "dependencies": {
    "global-store": "^1.0.0-beta.14",
    "is-node": "^1.0.2",
    "iso-error": "^2.2.0",
    "ms": "^2.1.2",
    "type-plus": "^1.26.2",
    "upper-case": "^1.1.3"
  },
  "devDependencies": {
    "assertron": "^7.1.2",
    "ramda": "^0.26.1",
    "satisfier": "^5.0.2"
  }
}
