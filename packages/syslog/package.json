{
  "name": "standard-log-syslog",
  "version": "5.0.1",
  "description": "syslog plugin for `standard-log`",
  "homepage": "https://github.com/unional/standard-log/tree/master/packages/syslog",
  "bugs": {
    "url": "https://github.com/unional/standard-log/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/unional/standard-log.git"
  },
  "license": "MIT",
  "author": {
    "name": "Homa Wong",
    "email": "homawong@gmail.com"
  },
  "sideEffects": false,
  "type": "module",
  "exports": {
    "import": "./esm/index.js",
    "require": "./cjs/index.js"
  },
  "main": "./cjs/index.js",
  "types": "./cjs/index.d.ts",
  "files": [
    "dist",
    "cjs",
    "esm",
    "ts",
    "!ts/**/*.spec.ts"
  ],
  "scripts": {
    "build": "npm-run-all build:clean -p build:cjs build:esm build:tslib bundle:es5 bundle:esm",
    "build:clean": "rimraf .tsbuildinfo || true",
    "build:cjs": "tsc -p ./tsconfig.cjs.json",
    "build:esm": "tsc -p ./tsconfig.esm.json",
    "build:tslib": "tsc -p ./tsconfig.tslib.json",
    "bundle": "run-p bundle:es5 bundle:esm",
    "bundle:es5": "webpack -c ./webpack.es5.cjs",
    "bundle:esm": "webpack -c ./webpack.esm.cjs",
    "clean": "rimraf cjs coverage dist esm lib libm tslib .tsbuildinfo && mkdir cjs && echo '{\"type\":\"commonjs\"}' > cjs/package.json || true",
    "coverage": "jest --coverage",
    "depcheck": "depcheck",
    "lint": "cross-env TIMING=1 eslint --ext=ts,js,cjs,mjs .",
    "prepublishOnly": "yarn build && yarn bundle",
    "size": "size-limit",
    "test": "jest",
    "test:watch": "jest --watch",
    "verify": "npm-run-all clean -p build depcheck lint coverage -p size",
    "watch": "jest --watch"
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "^7.0.8",
    "@types/ramda": "^0.28.13",
    "compression-webpack-plugin": "^10.0.0",
    "cross-env": "^7.0.3",
    "depcheck": "^1.4.3",
    "eslint": "^8.17.0",
    "jest": "^28.1.1",
    "npm-run-all": "^4.1.5",
    "param-case": "^3.0.4",
    "pascal-case": "^3.1.2",
    "ramda": "^0.28.0",
    "rimraf": "^3.0.2",
    "size-limit": "^7.0.8",
    "standard-log": "workspace:*",
    "ts-jest": "^28.0.5",
    "ts-loader": "^9.3.0",
    "tslib": "^2.4.0",
    "typescript": "^4.7.3",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0"
  },
  "peerDependencies": {
    "standard-log": ">=9.1.5"
  },
  "size-limit": [
    {
      "path": "./cjs/index.js",
      "limit": "10 kB"
    },
    {
      "path": "./esm/index.js",
      "limit": "10 kB"
    },
    {
      "path": "./tslib/index.js",
      "limit": "10 kB"
    }
  ]
}
